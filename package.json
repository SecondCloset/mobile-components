{
  "name": "@secondcloset/mobile-components",
  "version": "1.13.0-beta-6",
  "description": "Second Closet React Native Component Library",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "prestorybook": "rnstl --searchDir ./src --pattern '**/*.stories.tsx'",
    "storybook": "start-storybook -p 5694",
    "build-static-webapp": "expo build:web",
    "prepublishOnly": "sed -i '' -e $'/\"files\": / i\\\n  \\ \\ \"main\": \"dist/index.js\",\n' ./package.json && rm -rf dist && tsc",
