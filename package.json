{
  "name": "@secondcloset/mobile-components",
  "version": "1.6.11",
  "description": "Second Closet React Native Component Library",
  "scripts": {
    "start": "sed -i '' '/\"main\": \"dist\\/index.js\",/d' ./package.json && expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "prestorybook": "rnstl --searchDir ./src --pattern '**/*.stories.tsx'",
    "storybook": "start-storybook -p 5694",
    "prepare": "sed -i '' '/\"main\": \"dist\\/index.js\",/d' ./package.json && sed -i '' -e $'/\"files\": / i\\\n  \\ \\ \"main\": \"dist/index.js\",\n' ./package.json && rm -rf dist && tsc",
    "postpublish": "sed -i '' '/\"main\": \"dist\\/index.js\",/d' ./package.json"
  },
  "files": [
    "dist"
  ],
  "typings": "dist/index.d.ts",
  "author": "Anthony Langford <anthony.langford@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "expo-camera": "^8.0.0",
    "expo-image-manipulator": "^8.0.0",
    "expo-image-picker": "^8.0.2",
    "expo-sensors": "^8.0.0",
    "react-native-status-bar-height": "^2.4.0",
    "styled-components": "^5.0.0",
    "typescript-styled-plugin": "^0.15.0"
  },
  "devDependencies": {
    "@babel/core": "^7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.8.3",
    "@babel/preset-typescript": "^7.8.3",
    "@storybook/addon-actions": "^5.3.3",
    "@storybook/addon-links": "^5.3.3",
    "@storybook/addons": "^5.3.3",
    "@storybook/react-native": "^5.3.3",
    "@storybook/react-native-server": "^5.3.3",
    "@types/node": "^13.1.6",
    "@types/react": "~16.9.0",
    "@types/react-native": "~0.60.23",
    "@types/styled-components": "^4.4.2",
    "@typescript-eslint/eslint-plugin": "^2.16.0",
    "@typescript-eslint/parser": "^2.16.0",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "babel-plugin-import": "^1.13.0",
    "babel-preset-expo": "^8.0.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-config-react-app": "^5.1.0",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react-native": "^3.8.1",
    "expo": "~36.0.0",
    "prettier": "^1.19.1",
    "react": "^16.9.0",
    "react-dom": "~16.9.0",
    "react-native": "https://github.com/expo/react-native/archive/sdk-36.0.0.tar.gz",
    "react-native-screens": "2.0.0-alpha.12",
    "react-native-storybook-loader": "^1.8.1",
    "react-native-web": "~0.11.7",
    "typescript": "^3.7.4"
  },
